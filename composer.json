{
    "name": "nexendrie/utils",
    "description": "Collection of various utilities for your PHP project",
    "keywords": [
      "utils", "collections", "intervals", "constants", "numbers", "range"
    ],
    "license": "BSD-3-Clause",
    "homepage": "https://nexendrie.gitlab.io",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "docs": "https://nexendrie.gitlab.io/utils"
    },
    "require": {
        "php": ">=8.1.0"
    },
    "require-dev": {
        "nexendrie/code-quality": "dev-master",
        "nexendrie/site-generator": "^0.8"
    },
    "autoload": {
        "psr-4": {
            "Nexendrie\\Utils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexendrie\\Utils\\": "tests/Nexendrie/Utils/"
        }
    },
    "config": {
        "classmap-authoritative": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.9.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
