{
    "name": "nexendrie/utils",
    "description": "Collection of various utilities for your PHP project",
    "keywords": [
      "utils", "collections", "intervals"
    ],
    "license": "BSD-3-Clause",
    "homepage": "https://nexendrie.gitlab.io",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/nexendrie/utils/",
        "issues": "https://gitlab.com/nexendrie/utils/issues",
        "docs": "https://nexendrie.gitlab.io/utils"
    },
    "require": {
        "php": ">=7.1.0",
        "nette/utils": "^2.4"
    },
    "require-dev": {
        "nexendrie/code-quality": "dev-master",
        "nexendrie/site-generator": "1.0.x-dev",
        "phpstan/phpstan-nette": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "Nexendrie\\Utils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexendrie\\Utils\\": "tests/"
        }
    },
    "config": {
        "classmap-authoritative": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
